    "prepush:oppija": "npm run prepush --prefix oppija",
    "prepush:tyopaikantoimija": "npm run prepush --prefix tyopaikantoimija",
    "prepush": "npm-run-all prepush:*",
    "ci": "npm-run-all prettier:check lint test:* build:*"
