{
  "name": "react-typescript-webpack2",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --history-api-fallback --compress",
    "lint": "tslint src/*.tsx src/**/*.tsx",
    "test": "jest",
    "build": "webpack --config webpack.config.js --mode production"
  },
  "private": true,
  "author": "Lari Hoppula",
  "license": "MIT",
  "devDependencies": {
    "@types/webpack-env": "^1.13.6",
    "awesome-typescript-loader": "^5.2.0",
    "file-loader": "^2.0.0",
    "jest": "^23.5.0",
    "prettier": "^1.14.2",
    "react-test-renderer": "^16.4.2",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "@reach/router": "^1.1.1",
    "@types/jest": "^23.3.1",
    "@types/lodash.camelcase": "^4.3.4",
    "@types/lodash.kebabcase": "^4.1.4",
    "@types/map-obj": "^2.0.0",
    "@types/reach__router": "^1.0.0",
    "@types/react": "^16.4.10",
    "@types/react-circular-progressbar": "^1.0.1",
    "@types/react-dom": "^16.0.7",
    "@types/react-intl": "^2.3.9",
    "@types/react-loadable": "^5.4.1",
    "@types/react-test-renderer": "^16.0.2",
    "emotion": "^9.2.6",
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1",
    "map-obj": "^3.0.0",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.8",
    "mobx-state-tree": "^3.2.3",
    "react": "^16.5.0",
    "react-circular-progressbar": "^1.0.0",
    "react-dom": "^16.5.0",
    "react-emotion": "^9.2.6",
    "react-icons": "^3.0.5",
    "react-intl": "^2.4.0",
    "react-loadable": "^5.5.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "components(.*)$": "<rootDir>/src/components$1",
      "models(.*)$": "<rootDir>/src/models$1",
      "routes(.*)$": "<rootDir>/src/routes$1",
      "stores(.*)$": "<rootDir>/src/stores$1",
      "^config$": "<rootDir>/src/config",
      "^utils$": "<rootDir>/src/utils",
      "^testUtils$": "<rootDir>/src/testUtils",
      "\\.(css|jpg|png|svg)$": "<rootDir>/empty-module.js"
    }
  }
}
